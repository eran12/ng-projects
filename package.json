{
  "name": "ng-projects",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "serve": "yarn run build_dynamic_table && yarn add dynamic-table && yarn ng serve",
    "start": "yarn ng serve",
    "build": "yarn ng build",
    "test": "yarn ng test",
    "lint": "yarn ng lint",
    "e2e": "yarn ng e2e",
    "build_dynamic_table": "yarn ng build dynamicTable",
    "build_dynamic_table_prod": "yarn ng build dynamicTable --prod",
    "pack_dynamic_table": "cd dist/dynamic-table",
    "build_pack_dynamic_table": "yarn run build_dynamic_table_prod && yarn pack_dynamic_table"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.5",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/forms": "~10.1.5",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/router": "~10.1.5",
    "bootstrap": "^4.5.3",
    "dynamic-table": "^1.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.5",
    "@angular/cli": "~10.1.5",
    "@angular/compiler-cli": "~10.1.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
